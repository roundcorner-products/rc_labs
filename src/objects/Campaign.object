    </validationRules>
    <validationRules>
        <fullName>rC_Giving__Giving_Type_Invalid</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(CASE(rC_Giving__Giving_Type__c, &apos;&apos;, 1, &apos;New&apos;, 1, &apos;Additional&apos;, 1, &apos;Upgrade&apos;, 1, &apos;Renewal&apos;, 1, &apos;Rejoin&apos;, 1, NULL)))</errorConditionFormula>
