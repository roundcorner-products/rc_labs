        <formula>IF(AND(ISBLANK(rC_Connect__Street_Line_1__c), ISBLANK(rC_Connect__Street_Line_2__c), ISBLANK(rC_Connect__City__c), ISBLANK(rC_Connect__State__c),ISBLANK(rC_Connect__Postal_Code__c),ISBLANK(rC_Connect__Country__c)),
 &apos;&apos; ,
IF(rC_Connect__Street_Line_1__c ==&apos;&apos;, &apos;&apos; ,
IF( AND(ISBLANK(rC_Connect__Street_Line_2__c), ISBLANK(rC_Connect__City__c), ISBLANK(rC_Connect__State__c),ISBLANK(rC_Connect__Postal_Code__c),ISBLANK(rC_Connect__Country__c))
,rC_Connect__Street_Line_1__c,rC_Connect__Street_Line_1__c&amp;&apos;, &apos;) )&amp;

IF(rC_Connect__Street_Line_2__c==&apos;&apos; ,&apos;&apos;,
IF(AND(ISBLANK(rC_Connect__City__c), ISBLANK(rC_Connect__State__c),ISBLANK(rC_Connect__Postal_Code__c),ISBLANK(rC_Connect__Country__c))
,rC_Connect__Street_Line_2__c,rC_Connect__Street_Line_2__c&amp;&apos;, &apos;))&amp;

IF(rC_Connect__City__c ==&apos;&apos;,&apos;&apos;,
IF(AND(ISBLANK(rC_Connect__State__c),ISBLANK(rC_Connect__Postal_Code__c),ISBLANK(rC_Connect__Country__c))
 ,rC_Connect__City__c,rC_Connect__City__c &amp;&apos;, &apos;))&amp;
    
IF(rC_Connect__State__c==&apos;&apos;,&apos;&apos;,
IF(AND(ISBLANK(rC_Connect__Postal_Code__c),ISBLANK(rC_Connect__Country__c))
,rC_Connect__State__c,rC_Connect__State__c&amp; &apos;, &apos;))&amp;

IF(rC_Connect__Postal_Code__c==&apos;&apos;,&apos;&apos;,
IF(ISBLANK(rC_Connect__Country__c),rC_Connect__Postal_Code__c,rC_Connect__Postal_Code__c&amp; &apos;, &apos;))&amp;

IF(rC_Connect__Country__c==&apos;&apos;, &apos;&apos; , rC_Connect__Country__c)

)</formula>
